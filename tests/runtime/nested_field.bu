class Foo {
    a: i32;
    f: Foo;
    constructor() {
        this.a = 5;
        this.f = this;
    }

    func getA() -> i32 {
        return this.a;
    }

    func getF() -> Foo {
        return this.f;
    }
}

func main() -> i32 {
    let f: Foo = Foo();
    let result: i32 = f.f.f.getF().f.f.f.f.getA() + 3;
    if (result != 8) {
        return 1;
    }
    return 0;
}