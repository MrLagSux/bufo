class Tree {
    left: Tree;
    right: Tree;
    value: u32;

    constructor(value: u32) {
        this.value = value;
    }
    func setLeft(this, left: Tree) {
        this.left = left;
    }
    func setRight(this, right: Tree) {
        this.right = right;
    }
}

func main() -> u32 {
    let root: Tree = Tree(0);
    let left: Tree = Tree(1);
    let right: Tree = Tree(2);
    root.setLeft(left);
    root.setRight(right);
    let value: u32 = root.left.value + root.right.value;
    if (value != 3) {
        return value;
    }
    return 0;
}