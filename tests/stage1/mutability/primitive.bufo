func foo(mut a: i32) -> i32 {
    a = 10;
    return a;
}

func bar(a: i32) -> i32 {
    if (a == 10) return 5;
    return 2;
}

func main() -> i32 {
    let a: i32 = 5;
    let b: i32 = foo(a);
    if (a == 10) return 1;
    if (b != 10) return 2;
    mut c: i32 = 10;
    c = bar(c);
    if (c != 5) return 3;
    if (a + b + c != 20) return 4;
    return 0;
}