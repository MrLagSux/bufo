func main() -> u64 {
    mut counter: u64 = 0;
    mut a: u64 = 0;
    while (a < 100) {
        mut b: u64 = 0;
        while (b < 100) {
            counter = counter + 1;
            b = b + 1;
        }
        a = a + 1;
    }
    if (counter != 10000) return 1;
    return 0;
}