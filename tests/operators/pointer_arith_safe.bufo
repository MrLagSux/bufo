func main() -> i32 {
    mut a: i32 = 5;
    let b: &mut i32 = &mut a;
    unsafe {
        *(b + 5 - 5) = 10;
    }
    assert_with_msg(a == 10, "a is not 10!");
    unsafe {
        *(5 + b - 5) = 20;
    }
    assert_with_msg(a == 20, "a is not 20!");
    return 0;
}
