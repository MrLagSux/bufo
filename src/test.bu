struct Idee {
    a: u64
}
struct Bla {
    a: usize,
    b: i32,
    d: Idee,
}

struct Test {
    f: u64,
    b: i32,
    c: Bla,
}

func bla(t: Test) -> Bla {
    let a: u64 = t.c.d.a;
    let b: Bla = Bla {
        a: 0,
        b: 0,
        d: Idee {
            a: a + a
        }
    };
    return b;
}

func main() {
    let a: u64[3] = [
        1, 2, 3
    ];

    let ideeArr: Idee[3] = [
        Idee {
            a: 500
        },
        Idee {
            a: 10
        },
        Idee {
            a: 5
        }
    ];
    let t: Test = Test {
        f: 5,
        b: 10,
        c: Bla {
            a: 0,
            b: 0,
            d: Idee {
                a: a[1] + 5
            },
        }
    };
    t.c.d = ideeArr[0];
    t.c.d.a = t.f;
    t.f = 50;
}