struct Idee {
    a: i32
}
struct Bla {
    d: Idee,
}

struct Test {
    f: i32,
    b: i32,
    c: Bla,
}

func main() {
    let a: i32[3] = [
        1, 2, 3
    ];
    let t: Test = Test {
        f: 5,
        b: 10,
        c: Bla {
            d: Idee {
                a: a[1] + 5
            },
        }
    };
    t.c.d.a = t.b - t.f;
    t.f = 50;
}